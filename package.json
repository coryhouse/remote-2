{
  "name": "app2",
  "version": "0.0.0",
  "private": true,
  "devDependencies": {
    "@types/node": "^15.0.3",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "cross-env": "^7.0.3",
    "html-webpack-plugin": "5.3.1 ",
    "serve": "14.2.0",
    "surge": "^0.23.1",
    "ts-loader": "^9.1.2",
    "typescript": "^5.1.3",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.3"
  },
  "scripts": {
    "start": "webpack serve",
    "build": "webpack --mode production && cd dist && cp index.html 200.html",
